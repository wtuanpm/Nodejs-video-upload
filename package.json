{
  "name": "site-plan-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --silent --detectOpenHandles --forceExit ",
    "deploy:dev": "shipit develop deploy --shipitfile ./shipitfile.js",
    "deploy:prod": "shipit production deploy --shipitfile ./shipitfile.production.js",
    "deploy:staging": "shipit develop deploy --shipitfile ./shipitfile.staging.js",
    "gen:types": "graphql-codegen",
    "build": "rimraf dist && tsc --project tsconfig.json",
    "dev": "yarn build && concurrently -n HTTP,WORKER -c white,blue \"yarn server:api\" \"NODE_ENV=development && yarn server:worker\"",
    "server:test": "yarn nodemon",
    "staging": "yarn lint && yarn build && concurrently -n HTTP,WORKER -c white,blue \"yarn staging:api\" \"NODE_ENV=development && yarn server:worker\"",
    "wait-on": "wait-on dist/src/index.js -d 2000",
    "nodemon": "SET NODE_ENV=development && nodemon",
    "server:api": "yarn wait-on && NODE_ENV=development node dist/src/index.js",
    "staging:api": "yarn wait-on && NODE_ENV=staging node dist/src/index.js",
    "start": "yarn lint && yarn build && concurrently -n HTTP,WORKER -c white,blue \"yarn wait-on && NODE_ENV=production node dist/src/index.js\" \"NODE_ENV=production && yarn server:worker\"",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli -f s./ormconfig.ts migration:create -n",
    "migration:run": "yarn typeorm:cli -- migration:run",
    "lint": "./node_modules/.bin/eslint .eslintrc.js",
    "postbuild": "cpy src/graphql/schema/*.graphql dist/src/graphql/schema",
    "server:worker": "yarn wait-on &&  DEBUG=WORKER node dist/src/services/worker/index.js"
  },
